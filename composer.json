{
    "name": "ruthgeridema/simplicate-php",
    "description": "Simplcate PHP client",
    "keywords": [
        "simplicate"
    ],
    "homepage": "https://github.com/ruthgeridema/simplicate-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Ruthger Idema",
            "email": "ruthger.idema@gmail.com",
            "homepage": "https://ide.ma"
        }
    ],
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "symfony/var-dumper": "^4.3",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "RuthgerIdema\\Simplicate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RuthgerIdema\\Simplicate\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
